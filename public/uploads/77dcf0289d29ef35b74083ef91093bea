function Ability (name, image, power=1, soundEffect=null, text="FROG") {
    this.name = name;
    this.image = image;
    this.soundEffect = soundEffect;
    this.power = power;
    this.text = text;
}



var swordAttack = new Ability("Sword Attack", "IMAGE", 1);

var shieldDefence = new Ability("Shield Defence", "IMAGE", 1);

var prayer = new Ability("Prayer", "IMAGE", 1);

// Stark

var summonBabyDirewolf = new Ability("Summon Baby Direwolf", "IMAGE", 1, ".direwolf");

