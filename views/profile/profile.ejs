
<div>
  <ul>
    Username: <%= user.username %>
  </ul>
  <ul>
    My projects:
    <% if (user.projectIDs.length === 0) { %>
      <li>No projects</li>
    <% } else { %>
      <% user.projectIDs.forEach((project) => { %>
        <li><a href="/project/<%= project._id %>"> <%=project.projectName %></a></li>
      <% }); %>
    <% } %>
  </ul>
</div>
